{% extends 'base.html.twig' %}

{% block title %}Concert{% endblock %}

{% block body %}
    <h1>Concert</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ concert.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ concert.Name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ concert.Description }}</td>
            </tr>
            <tr>
                <th>StartAt</th>
                <td>{{ concert.StartAt ? concert.StartAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EndAt</th>
                <td>{{ concert.EndAt ? concert.EndAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ concert.Type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_concert_index') }}">back to list</a>

    <a href="{{ path('app_concert_edit', {'id': concert.id}) }}">edit</a>

    {{ include('concert/_delete_form.html.twig') }}

            
